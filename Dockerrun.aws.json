{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "modolee/multi-docker-frontend",
      "hostname": "frontend",
      "essential": false
    },
    {
      "name": "api",
      "image": "modolee/multi-docker-api",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "modolee/multi-docker-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "modolee/multi-docker-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["frontend", "api"]
    }
  ]
}